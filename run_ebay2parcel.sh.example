#!/bin/bash
# eBay2Parcel cron wrapper script
#
# Copy this file to run_ebay2parcel.sh and update the path:
#   cp run_ebay2parcel.sh.example run_ebay2parcel.sh
#   chmod +x run_ebay2parcel.sh
#
# Then add to crontab (crontab -e):
#   15 3 * * * /path/to/eBay2Parcel/run_ebay2parcel.sh >> /path/to/eBay2Parcel/cron.log 2>&1

cd /path/to/eBay2Parcel
/path/to/eBay2Parcel/venv/bin/python main.py
